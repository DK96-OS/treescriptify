<?xml version='1.0' encoding='utf-8'?>
<sorting>
    <changelist name="TreeScriptify Data Package" module="module">
        <files path_start="treescriptify/data" />
    </changelist>
    <changelist name="TreeScriptify Input Package" module="module">
        <files path_start="treescriptify/input/" />
    </changelist>
    <changelist name="TreeScriptify Package" module="module">
        <files first_dir="treescriptify" />
    </changelist>
     <changelist name="Root Project Python Files" module="root">
        <files file_ext="py" />
    </changelist>
    <changelist name="Data Package Tests" module="module">
        <files first_dir="test" />
        <files path_end="data" />
    </changelist>
    <changelist name="Input Package Tests" module="module">
        <files first_dir="test" />
        <files path_end="input" />
    </changelist>
    <changelist name="Test Fixtures and Data Providers" module="module">
        <files first_dir="test" />
        <files filename_prefix="conftest" />
        <files file_ext="py" />
    </changelist>
    <changelist name="TreeScriptify Package Tests" module="module">
        <files path_start="test/treescriptify/" />
    </changelist>
    <changelist name="Changelists Config" module="hidden">
        <files first_dir=".changelists" />
        <files file_ext="xml" />
    </changelist>
    
    <changelist name="GitHub Workflows" module="hidden">
        <files first_dir=".github" />
        <files path_end="workflows" />
        <files file_ext="yml" />
    </changelist>
    <changelist name="GitHub Dependabot" module="hidden">
        <files first_dir=".github" />
        <files filename_prefix="dependabot" />
        <files file_ext="yml" />
    </changelist>
    
    <changelist name="Project Root">
        <files first_dir="None" />
    </changelist>
</sorting>